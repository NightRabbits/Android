<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".StudyHiWidgetActivity">

    <Button
        android:id="@+id/bt_TsetTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onTestTimeClick"
        android:text="TestTime" />

    <Button
        android:id="@+id/bt_TestSv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onTestSvClick"
        android:text="TestSV" />

    <Button
        android:id="@+id/bt_TestSDraw"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onTestSDrawClick"
        android:text="TestSDraw" />

    <Button
        android:id="@+id/bt_TestFlipper"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onTestFlipperClick"
        android:text="TestFlipper" />
</LinearLayout>